{
	"metadata": {
		"version": 1
	},
	"application": {
		"name": "TestDotNetComponent",
		"language": "DOTNET",
		"artifact_store": {
			"type": "azure-blob",
			"repositories": [
				{
					"category": "rc-packaging",
					"bucket": "guide-rails-rc"
				},
				{
					"category": "final-packaging",
					"bucket": "guide-rails-final"
				},
				{
					"category": "logs",
					"bucket": "guide-rails-logs"
				}
			]
		}
	},
	"pipeline": {
		"segments": [
			{
				"name": "Build",
				"segment_type": "build",
				"job_steps": [
					{
						"name": "Restore",
						"definition": "Restore with nuget",
						"command": "nuget restore",
						"properties": null
					},
					{
						"name": "Build",
						"definition": "Build debug files",
						"command": "msbuild /t:Rebuild /p:Configuration=Debug",
						"properties": null
					},
					{
						"name": "Unit test and Coverage",
						"definition": "Unit test and coverage",
						"command": "dotnet test .\\MyProject.Test --logger \"trx;LogFileName=TestResults.trx\"\nCopy-Item -Path .\\MyProject.Test\\TestResults\\TestResults.trx -Destination .\\testresults.trx -Force -ErrorAction SilentlyContinue\ndotnet test .\\MyProject.Test /p:CollectCoverage=true /p:CoverletOutputFormat=opencover\nCopy-Item -Path .\\MyProject.Test\\coverage.opencover.xml -Destination .\\coverage.xml -Force -ErrorAction SilentlyContinue",
						"properties": null
					}
				],
				"exit_actions": {
					"default": {
						"fetch_logs": true,
						"code_analysis": {
							"publish": true,
							"properties": null
						},
						"delete_instances": true
					},
					"on_success": {
						"code_analysis": null
					},
					"on_failure": {
						"code_analysis": null
					},
					"on_abort": {
						"code_analysis": null
					}
				},
				"deployment_strategy": {},
				"application_init": {
					"command": "",
					"arguments": null
				},
				"packages": [
					{
						"name": "((application.name))",
						"dependencies": [],
						"downloads": [],
						"files": [
							"((application.name))*"
						],
						"package_install_command": "",
						"installed_path": ""
					}
				],
				"instance_tags": [
					{
						"name": "application.name",
						"value": "((application.name))"
					}
				],
				"post_deployment_actions": {
					"instances": null,
					"loadbalanced": false,
					"loadbalancer": {
						"TargetGroupName": "",
						"TargetGroupPort": 0,
						"TargetGroupProtocol": "",
						"TargetType": "",
						"VPCID": "",
						"HealthCheckEnabled": false,
						"HealthCheckPort": "",
						"UnhealthyThresholdCount": 0
					}
				},
				"cache_info": {}
			}
		],
		"exit_actions": {
			"default": {
				"fetch_logs": true,
				"code_analysis": {
					"publish": true,
					"properties": null
				},
				"delete_instances": true
			},
			"on_success": {
				"code_analysis": null
			},
			"on_failure": {
				"code_analysis": null
			},
			"on_abort": {
				"code_analysis": null
			}
		},
		"default_properties": [
			{
				"name": "source_dirs",
				"value": "MyProject/*.cs",
				"type": "string"
			}
		],
		"deployment_files": [],
		"application_init": {
			"command": "",
			"arguments": null
		},
		"process": {
			"init": {
				"command": "bin/start-service.sh"
			}
		},
		"releases": []
	},
	"organization": {
